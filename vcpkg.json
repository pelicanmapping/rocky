{
    "name": "rocky",
    "version-string": "0.9",
    "port-version": 1,
    "description": "3D Maps SDK for C++/Vulkan",
    "homepage": "https://github.com/pelicanmapping/rocky",
    "supports": "!(x86 | wasm32)",
    "dependencies": [
        { "name" : "cpp-httplib" },
        { "name" : "entt" },
        { "name" : "gdal", "default-features": false, "features": [ "recommended-features", "webp" ] },
        { "name" : "glm" },
        { "name" : "imgui", "features": [ "vulkan-binding" ] },
        { "name" : "nlohmann-json" },
        { "name" : "openssl" },
        { "name" : "proj" },
        { "name" : "spdlog" },
        { "name" : "sqlite3" },
        { "name" : "vsg", "features" : [ "windowing" ] },
        { "name" : "vsgxchange", "features": [ "assimp", "freetype" ] },
        { "name" : "zlib" }
    ]
}
