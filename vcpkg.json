{
    "name": "rocky",
    "version-string": "0.9",
    "port-version": 1,
    "description": "Rocky by Pelican Mapping",
    "homepage": "https://github.com/pelicanmapping/rocky",
    "supports": "!(x86 | wasm32)",
    "dependencies": [
        "cpp-httplib",
        "entt",
        { "name": "gdal", 
          "default-features": false, 
          "features": [ "recommended-features", "webp" ] },
        "glm",
        { "name" : "imgui", "features": [ "vulkan-binding" ] },
        "nlohmann-json",
        "openssl",
        "proj",
        "qt5",
        "spdlog",
        "sqlite3",
        { "name" : "vsg", "features" : [ "windowing" ] },
        "vsgqt",
        { "name" : "vsgxchange", "features": [ "assimp", "freetype" ] },
        "zlib"
    ]
}
